{
    "opts": {
        "recurse": true,
        "destination": "dist/doc"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": [
        ],
        "include": [
          "src", "doc/es6.md"
        ]
    },
    "plugins": [
        "node_modules/jsdoc/plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true,
        "systemName": "JSTS",
        "copyright": "Bj√∂rn Harrtell",
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true,
            "useLongnameInNav": true
        },
        "applicationName": "JSTS"
    }
}
